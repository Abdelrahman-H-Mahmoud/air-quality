{
  "name": "air-quality-api",
  "version": "1.0.0",
  "description": "TypeScript Express Prisma Application",
  "main": "dist/index.js",
  "scripts": {
    "dev:server": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:worker": "ts-node-dev --respawn --transpile-only src/worker.ts",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:worker\"",
    "build": "tsc",
    "start:server": "node dist/index.js",
    "start:worker": "node dist/worker.js",
    "start": "concurrently \"npm run start:server\" \"npm run start:worker\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.8.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "node-cron": "^3.0.3",
    "mongoose": "^8.2.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@types/express": "^4.17.21",
    "@types/node-cron": "^3.0.11",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.11.24",
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "mongodb-memory-server": "^9.1.6",
    "supertest": "^6.3.4"
  }
}
